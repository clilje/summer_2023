}

@misc{harikane_jwstnirspec_2023,
	title = {{JWST}/{NIRSpec} {First} {Census} of {Broad}-{Line} {AGNs} at z=4-7: {Detection} of 10 {Faint} {AGNs} with {M}\_BH{\textasciitilde}10{\textasciicircum}6-10{\textasciicircum}7 {M}\_sun and {Their} {Host} {Galaxy} {Properties}},
	shorttitle = {{JWST}/{NIRSpec} {First} {Census} of {Broad}-{Line} {AGNs} at z=4-7},
	url = {http://arxiv.org/abs/2303.11946},
	abstract = {We present a first statistical sample of faint type-1 AGNs at \$z{\textgreater}4\$ identified by JWST/NIRSpec deep spectroscopy. Among the 185 galaxies at \$z\_{\textbackslash}mathrm\{spec\}=3.8-8.9\$ confirmed with NIRSpec, our systematic search for broad-line emission reveals 10 type-1 AGNs at \$z=4.015-6.936\$ whose broad component is only seen in the permitted H\${\textbackslash}alpha\$ line and not in the forbidden [OIII]\${\textbackslash}lambda\$5007 line that is detected with greater significance than H\${\textbackslash}alpha\$. The broad H\${\textbackslash}alpha\$ line widths of \${\textbackslash}mathrm\{FWHM\}{\textbackslash}simeq1000-6000{\textbackslash} {\textbackslash}mathrm\{km{\textbackslash} s{\textasciicircum}\{-1\}\}\$ suggest that the AGNs have low-mass black holes with \$M\_{\textbackslash}mathrm\{BH\}{\textbackslash}sim10{\textasciicircum}6-10{\textasciicircum}7{\textbackslash} M\_{\textbackslash}odot\$, remarkably lower than those of low-luminosity quasars previously identified at \$z{\textgreater}4\$ with ground-based telescopes. JWST and HST high-resolution images reveal that the majority of them show extended morphologies indicating significant contribution to the total lights from their host galaxies, except for three compact objects two of which show red SEDs, probably in a transition phase from faint AGNs to low luminosity quasars. Careful AGN-host decomposition analyses show that their host's stellar masses are systematically lower than the local relation between the black hole mass and the stellar mass, implying a fast black hole growth consistent with predictions from theoretical simulations. A high fraction of the broad-line AGNs (\${\textbackslash}sim5{\textbackslash}\%\$), higher than \$z{\textbackslash}sim0\$, indicates that a number density of such faint AGNs is higher than an extrapolation of the quasar luminosity function, implying a large population of AGNs including type 1 and type 2 in the early universe. Such faint AGNs contribute to cosmic reionization, while the total contribution is not large, up to \${\textbackslash}sim50{\textbackslash}\%\$ at \$z{\textbackslash}sim6\$, because of their faint nature.},
	urldate = {2023-06-20},
	publisher = {arXiv},
	author = {Harikane, Yuichi and Zhang, Yechi and Nakajima, Kimihiko and Ouchi, Masami and Isobe, Yuki and Ono, Yoshiaki and Hatano, Shun and Xu, Yi and Umeda, Hiroya},
	month = mar,
