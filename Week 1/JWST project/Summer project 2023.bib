@misc{kocevski_hidden_2023,
	title = {Hidden {Little} {Monsters}: {Spectroscopic} {Identification} of {Low}-{Mass}, {Broad}-{Line} {AGN} at \$z{\textgreater}5\$ with {CEERS}},
	shorttitle = {Hidden {Little} {Monsters}},
	url = {http://arxiv.org/abs/2302.00012},
	abstract = {We report on the discovery of two low-luminosity, broad-line AGN at \$z{\textgreater}5\$ identified using JWST NIRSpec spectroscopy from the CEERS Survey. We detect broad H\${\textbackslash}alpha\$ emission from both sources, with FWHM of \$2038{\textbackslash}pm286\$ and \$1807{\textbackslash}pm207\$ km s\${\textasciicircum}\{-1\}\$, resulting in black hole (BH) masses that are 1-2 dex below that of existing samples of luminous quasars at \$z{\textgreater}5\$. The first source, CEERS 1670 at \$z=5.242\$, is 2-3 dex fainter than known quasars at similar redshifts and was previously identified as a candidate low-luminosity AGN based on its rest-frame optical SED. We measure a BH mass of \$M\_\{{\textbackslash}rm BH\}=1.3{\textbackslash}pm0.4{\textbackslash}times 10{\textasciicircum}\{7\}{\textasciitilde}M\_\{{\textbackslash}odot\}\$, confirming that this AGN is powered by the least-massive BH known in the universe at the end of cosmic reionization. The second source, CEERS 3210 at \$z=5.624\$, is inferred to be a heavily obscured, broad-line AGN caught in a transition phase between a dust-obscured starburst and an unobscured quasar. We estimate its BH mass to be \$M\_\{{\textbackslash}rm BH\}{\textbackslash}simeq 0.9-4.7 {\textbackslash}times 10{\textasciicircum}\{7\}{\textasciitilde}M\_\{{\textbackslash}odot\}\$, depending on the level of dust obscuration assumed. We derive host stellar masses, \$M\_{\textbackslash}star\$, allowing us to place constraints on the BH-galaxy mass relationship in the lowest mass range yet probed in the early universe. The \$M\_\{{\textbackslash}rm BH\}/M\_{\textbackslash}star\$ ratio for CEERS 1670, in particular, is consistent with or higher than the empirical relationship seen in massive galaxies at \$z=0\$. We examine the emission-line ratios of both sources and find that their location on the BPT and OHNO diagrams is consistent with model predictions for low-metallicity AGN with \$Z/Z\_{\textbackslash}odot {\textbackslash}simeq 0.2-0.4\$. The spectroscopic identification of low-luminosity, broad-line AGN at \$z{\textgreater}5\$ with \$M\_\{{\textbackslash}rm BH\}{\textbackslash}simeq 10{\textasciicircum}\{7\}{\textasciitilde}M\_\{{\textbackslash}odot\}\$ demonstrates the capability of JWST to push BH masses closer to the range predicted for the BH seed population and provides a unique opportunity to study the early stages of BH-galaxy assembly.},
	urldate = {2023-06-20},
	publisher = {arXiv},
	author = {Kocevski, Dale D. and Onoue, Masafusa and Inayoshi, Kohei and Trump, Jonathan R. and Haro, Pablo Arrabal and Grazian, Andrea and Dickinson, Mark and Finkelstein, Steven L. and Kartaltepe, Jeyhan S. and Hirschmann, Michaela and Fujimoto, Seiji and Juneau, Stephanie and Amorin, Ricardo O. and Bagley, Micaela B. and Barro, Guillermo and Bell, Eric F. and Bisigello, Laura and Calabro, Antonello and Cleri, Nikko J. and Cooper, M. C. and Ding, Xuheng and Grogin, Norman A. and Ho, Luis C. and Inoue, Akio K. and Jiang, Linhua and Jones, Brenda and Koekemoer, Anton M. and Li, Wenxiu and Li, Zhengrong and McGrath, Elizabeth J. and Molina, Juan and Papovich, Casey and Perez-Gonzalez, Pablo G. and Pirzkal, Nor and Wilkins, Stephen M. and Yang, Guang and Yung, L. Y. Aaron},
	month = jan,
	year = {2023},
	note = {arXiv:2302.00012 [astro-ph]},
	keywords = {Astrophysics - Astrophysics of Galaxies},
	annote = {Comment: 16 pages, 8 figures, submitted to ApJL},
