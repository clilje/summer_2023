@misc{ono_morphologies_2023,
	title = {Morphologies of {Galaxies} at \$z {\textbackslash}gtrsim 9\$ {Uncovered} by {JWST}/{NIRCam} {Imaging}: {Cosmic} {Size} {Evolution} and an {Identification} of an {Extremely} {Compact} {Bright} {Galaxy} at \$z{\textbackslash}sim 12\$},
	shorttitle = {Morphologies of {Galaxies} at \$z {\textbackslash}gtrsim 9\$ {Uncovered} by {JWST}/{NIRCam} {Imaging}},
	url = {http://arxiv.org/abs/2208.13582},
	abstract = {We present morphologies of galaxies at \$z {\textbackslash}gtrsim 9\$ resolved by JWST/NIRCam \$2\$-\$5{\textbackslash}mu\$m imaging. Our sample consists of \$22\$ galaxy candidates identified by stringent dropout and photo-\$z\$ criteria in GLASS, CEERS, SMACS J0723, and Stephan's Quintet flanking fields, one of which has been spectroscopically identified at \$z=11.44\$. We perform surface brightness (SB) profile fitting with GALFIT for \$6\$ bright galaxies with S/N \$=10\$-\$40\$ on an individual basis and for stacked faint galaxies with secure point-spread functions (PSFs) of the NIRCam real data, carefully evaluating systematics by Monte-Carlo simulations. We compare our results with those of previous JWST studies, and confirm that effective radii \$r\_\{{\textbackslash}rm e\}\$ of our measurements are consistent with those of previous measurements at \$z{\textbackslash}sim 9\$. We obtain \$r\_\{{\textbackslash}rm e\}{\textbackslash}simeq 200\$-\$300\$ pc with the exponential-like profiles, S{\textbackslash}'ersic indexes of \$n{\textbackslash}simeq 1\$-\$1.5\$, for galaxies at \$z{\textbackslash}sim 12\$-\$16\$, indicating that the relation of \$r\_\{{\textbackslash}rm e\}{\textbackslash}propto (1+z){\textasciicircum}s\$ for \$s=-1.22{\textasciicircum}\{+0.17\}\_\{-0.16\}\$ explains cosmic evolution over \$z{\textbackslash}sim 0\$-\$16\$ for \${\textbackslash}sim L{\textasciicircum}*\_\{z=3\}\$ galaxies. One bright (\$M\_\{{\textbackslash}rm UV\}=-21\$ mag) galaxy at \$z{\textbackslash}sim 12\$, GL-z12-1, has an extremely compact profile with \$r\_\{{\textbackslash}rm e\}=39 {\textbackslash}pm 11\$ pc that is surely extended over the PSF. Even in the case that the GL-z12-1 SB is fit by AGN\$+\$galaxy composite profiles, the best-fit galaxy component is again compact, \$r\_\{{\textbackslash}rm e\}=48{\textasciicircum}\{+38\}\_\{-15\}\$ pc that is significantly (\${\textgreater}5{\textbackslash}sigma\$) smaller than the typical \$r\_\{{\textbackslash}rm e\}\$ value at \$z{\textbackslash}sim 12\$. Comparing with numerical simulations, we find that such a compact galaxy naturally forms at \$z{\textbackslash}gtrsim 10\$, and that frequent mergers at the early epoch produce more extended galaxies following the \$r\_\{{\textbackslash}rm e\}{\textbackslash}propto (1+z){\textasciicircum}s\$ relation.},
	urldate = {2023-06-20},
	publisher = {arXiv},
	author = {Ono, Yoshiaki and Harikane, Yuichi and Ouchi, Masami and Yajima, Hidenobu and Abe, Makito and Isobe, Yuki and Shibuya, Takatoshi and Wise, John H. and Zhang, Yechi and Nakajima, Kimihiko and Umeda, Hiroya},
	month = may,
