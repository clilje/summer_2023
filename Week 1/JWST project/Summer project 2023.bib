	file = {arXiv.org Snapshot:C\:\\Users\\clara\\Zotero\\storage\\P2IN39G6\\1210.html:text/html;Full Text PDF:C\:\\Users\\clara\\Zotero\\storage\\79QRXL9X\\Hao et al. - 2013 - Spectral Energy Distributions of Type 1 AGN in XMM.pdf:application/pdf},
}

@article{lyu_infrared_2022,
	title = {Infrared {Spectral} {Energy} {Distribution} and {Variability} of {Active} {Galactic} {Nuclei}: {Clues} to the {Structure} of {Circumnuclear} {Material}},
	volume = {8},
	issn = {2218-1997},
	shorttitle = {Infrared {Spectral} {Energy} {Distribution} and {Variability} of {Active} {Galactic} {Nuclei}},
	url = {http://arxiv.org/abs/2205.14172},
	doi = {10.3390/universe8060304},
	abstract = {The active galactic nucleus (AGN) phenomena results from a supermassive black hole accreting its surrounding gaseous and dusty material. The infrared (IR) regime provides most of the information to characterize the dusty structures that bridge from the galaxy to the black hole, providing clues to the black hole growth and host galaxy evolution. Over the past several decades, with the commissioning of various ground, airborne and space IR observing facilities, our interpretations of the AGN circumnuclear structures have advanced significantly through improved understanding of how their dust emission changes as a function of wavelength and how the heating of the dusty structures responds to variations of the energy released from the central engine. In this review, we summarize the current observational knowledge of the AGN IR broad-band spectral energy distributions (SEDs) and the IR time variability behavior covering large ranges of AGN luminosity and redshift, and discuss some first-order insights into the obscuring structures and host galaxy IR properties that can be obtained by integrating the relevant observations into a coherent picture.},
	number = {6},
	urldate = {2023-06-20},
	journal = {Universe},
	author = {Lyu, Jianwei and Rieke, George},
	month = may,
	year = {2022},
	note = {arXiv:2205.14172 [astro-ph]},
	keywords = {Astrophysics - Astrophysics of Galaxies, Astrophysics - Cosmology and Nongalactic Astrophysics},
	pages = {304},
	annote = {Comment: 50 pages, 17 figures; Invited review (in press) for the special issue "Recent Advances in Infrared Galaxies and AGN", edited by Anna Sajina and Asantha R. Cooray, in Universe},
	file = {arXiv.org Snapshot:C\:\\Users\\clara\\Zotero\\storage\\BJ9QFHNF\\2205.html:text/html;Full Text PDF:C\:\\Users\\clara\\Zotero\\storage\\D8HTVAYI\\Lyu und Rieke - 2022 - Infrared Spectral Energy Distribution and Variabil.pdf:application/pdf},
}

@misc{yang_ceers_2023-1,
	title = {{CEERS} {Key} {Paper} {VI}: {JWST}/{MIRI} {Uncovers} a {Large} {Population} of {Obscured} {AGN} at {High} {Redshifts}},
	shorttitle = {{CEERS} {Key} {Paper} {VI}},
	url = {http://arxiv.org/abs/2303.11736},
	abstract = {Mid-infrared observations are powerful in identifying heavily obscured Active Galactic Nuclei (AGN) which have weak emission in other wavelengths. Data from the Mid-Infrared Instrument (MIRI) onboard JWST provides an excellent opportunity to perform such studies. We take advantage of the MIRI imaging data from the Cosmic Evolution Early Release Science Survey (CEERS) to investigate the AGN population in the distant universe. We estimate the source properties of MIRI-selected objects by utilizing spectral energy distribution (SED) modelling, and classify them into star-forming galaxies (SF), SF-AGN mixed objects, and AGN. The source numbers of these types are 418, 111, and 31, respectively, from 4 MIRI pointings covering \${\textbackslash}sim 9\$ arcmin\${\textasciicircum}2\$. The sample spans a redshift range of \${\textbackslash}approx 0\$--5. We derive the median SEDs for all three source types, respectively, and publicly release them. The median MIRI SED of AGN is similar to the typical SEDs of hot dust-obscured galaxies and Seyfert 2s, for which the mid-IR SEDs are dominantly from AGN-heated hot dust. Based on our SED-fit results, we estimate the black-hole accretion density (BHAD; i.e., total BH growth rate per comoving volume) as a function of redshift. At \$z{\textless}3\$, the resulting BHAD agrees with the X-ray measurements in general. At \$z{\textgreater}3\$, we identify a total of 27 AGN and SF-AGN mixed objects, leading to that our high-\$z\$ BHAD is substantially higher than the X-ray results (\${\textbackslash}sim 0.5\$ dex at \$z {\textbackslash}approx 3\$--5). This difference indicates MIRI can identify a large population of heavily obscured AGN missed by X-ray surveys at high redshifts.},
	urldate = {2023-06-20},
	publisher = {arXiv},
	author = {Yang, G. and Caputi, K. I. and Papovich, C. and Haro, P. Arrabal and Bagley, M. B. and Behroozi, P. and Bell, E. F. and Bisigello, L. and Buat, V. and Burgarella, D. and Cheng, Y. and Cleri, N. J. and Dave, R. and Dickinson, M. and Elbaz, D. and Ferguson, H. C. and Finkelstein, S. L. and Grogin, N. A. and Hathi, N. P. and Hirschmann, M. and Holwerda, B. W. and Huertas-Company, M. and Hutchison, T. and Iani, E. and Kartaltepe, J. S. and Kirkpatrick, A. and Kocevski, D. D. and Koekemoer, A. M. and Kokorev, V. and Larson, R. L. and Lucas, R. A. and Perez-Gonzalez, P. G. and Rinaldi, P. and Shen, L. and Trump, J. R. and de la Vega, A. and Yung, L. Y. A. and Zavala, J. A.},
	month = may,
	year = {2023},
	note = {arXiv:2303.11736 [astro-ph]},
	keywords = {Astrophysics - Astrophysics of Galaxies},
	annote = {Comment: ApJL in press, as part of the CEERS Focus Issue. 13 pages and 7 figures (median SEDs in Fig. 5 are available upon request)},
	file = {arXiv.org Snapshot:C\:\\Users\\clara\\Zotero\\storage\\9P9ISHWP\\2303.html:text/html;Full Text PDF:C\:\\Users\\clara\\Zotero\\storage\\7RCJWCHV\\Yang et al. - 2023 - CEERS Key Paper VI JWSTMIRI Uncovers a Large Pop.pdf:application/pdf},
}

